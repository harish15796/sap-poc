	<sap:sap-config name="SAP_Config" doc:name="SAP Config" doc:id="ac30ceab-9234-49f2-a5cb-c01685effda1" >
		<sap:simple-connection-provider-connection username="MULEUSER" password="Welcome@123" systemNumber="00" client="500" applicationServerHost="sqs6ci.isidc.com">
			<sap:extended-properties >
				<sap:extended-property key="Djco.trace_level" value="true" />
			</sap:extended-properties>
		</sap:simple-connection-provider-connection>
	</sap:sap-config>
	<http:listener-config name="HTTP_Listener_config" doc:name="HTTP Listener config" doc:id="02f31d5f-b023-4996-9f3b-b07dc01d1686" >
		<http:listener-connection host="0.0.0.0" port="8081" />
	</http:listener-config>
	<flow name="sap-pocFlow" doc:id="aba82d95-8342-4f24-af2a-ba34fc7438ab" >
		<http:listener doc:name="Listener" doc:id="60d1e781-154b-4013-b665-bcdb62960c3e" config-ref="HTTP_Listener_config" path="sap"/>
		<ee:transform doc:name="Transform Message" doc:id="7d3395a9-6c61-43f0-bcb8-26874d7a35ae" >
			<ee:message >
				<ee:set-payload ><![CDATA[%dw 2.0
