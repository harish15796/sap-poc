		<ee:transform doc:name="Transform Message" doc:id="134498f0-7433-49ec-8e08-ce7f7d500991" >
			<ee:message >
				<ee:set-payload ><![CDATA[%dw 2.0
output application/xml
---
payload]]></ee:set-payload>
			</ee:message>
		</ee:transform>
		<logger level="INFO" doc:name="Logger" doc:id="8ca660f7-9afa-4152-8027-5011010584cc" message="flow ends!!"/>
